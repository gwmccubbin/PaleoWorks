      .panel-heading
        h3 Customer Info
      .panel-body
        table.panel-table
          tbody
            tr
              td.left
                label First Name: 
              td.right
                .input-group
                    span.input-group-addon
                      span.glyphicon.glyphicon-user
                    input.form-control placeholder="First Name" type="text"
            tr
              td.left
